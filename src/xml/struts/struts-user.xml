<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="user" extends="struts-default" >
		<action name="user*" class="com.baidu.user.action.UserAction" method="{1}">
			<result name="list">/WEB-INF/user/user_list.jsp</result>
			<result name="tree">/WEB-INF/user/tree.jsp</result>
			<result name="addPage">/WEB-INF/user/user_add.jsp</result>
			<result name="updatePage">/WEB-INF/user/user_update.jsp</result>
			<result name="userTree">/WEB-INF/user/user_tree.jsp</result>
		</action>
	</package>
</struts>